        "@types/node-dijkstra": "^2.5.6",
        "@types/react": "^17.0.6",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "dotenv": "^16.4.7",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^10.0.1",
        "jest": "^29.7.0",
        "jest-environment-node": "^29.7.0",
        "lint-staged": "^11.1.1",
        "prettier": "^2.3.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^9.1.1",
        "typescript": "^5.7.3"
