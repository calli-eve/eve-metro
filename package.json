    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "generate-static": "tsc --outDir dist src/pathfinder/static-nodes.ts && node -e 'require(\"./dist/pathfinder/static-nodes.js\").generateStaticData()' ",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "initialize-database": "node ./knex/init-db.js",
        "knex-migrate": "knex migrate:latest",
        "prettier-fix": "prettier --write ./**/*.{js,ts,tsx,css,md}",
        "prettier-check": "prettier --check ./**/*.{js,ts,tsx,css,md}",
        "test": "jest",
        "test:integration": "jest --config jest.integration.config.js --detectOpenHandles",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "antd": "^4.24.15",
        "apicache": "^1.6.3",
        "axios": "^0.21.1",
        "better-sqlite3": "^11.8.0",
        "crypto-js": "^4.2.0",
        "eslint-config-next": "^12.3.4",
        "knex": "^3.1.0",
        "luxon": "^1.27.0",
        "morgan": "^1.10.0",
        "next": "^12.3.4",
        "next-connect": "^0.9.1",
        "next-iron-session": "^4.2.0",
        "node-dijkstra": "^2.5.0",
        "pg": "^8.13.1",
        "query-string": "^9.1.1",
        "ramda": "^0.30.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-graph-vis": "^1.0.7",
        "react-hot-toast": "^2.5.1",
        "react-json-tree": "^0.19.0",
        "react-responsive": "^10.0.0",
        "recharts": "^2.15.0",
        "sharp": "^0.33.5",
        "unstated-next": "^1.1.0",
        "xmldom": "^0.6.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
